                                                    </div>
                                                    <span class="remove-link">
                                                        <a style="cursor:pointer;" onclick="removeCartHome('@item.Product.Product_ID')"><i class="fa fa-times-circle" aria-hidden="true"></i></a>
                                                    </span>
                                                </dt>
                                                <dd></dd>
                                            }
